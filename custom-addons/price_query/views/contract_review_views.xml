<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
    
        <record id="contract_review_view_form_inherit_datasheet" model="ir.ui.view">
            <field name="name">sale.order.view.form.inherit</field>
            <field name="model">sale.order</field>
            <field name="inherit_id" ref="crm_management.contract_review_view_form"/>
            <field name="arch" type="xml">
                <xpath expr="//page[@name='order_lines']//field[@name='order_line']//tree" position="inside">
                    <field name="pq_line_id" optional="hide" />
                    <button name="action_show_datasheet_pq" type="object" icon="fa-object-group" title="Datasheet" width="0.1" options="{&quot;warn&quot;: true}"/>
                </xpath>
            </field>
        </record>
    
    </data>

</odoo>
