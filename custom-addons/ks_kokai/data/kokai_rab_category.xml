<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <!-- Main Categories -->
        <record id="rab_category_material" model="kokai.rab.category">
            <field name="name">Material</field>
            <field name="code">MAT</field>
            <field name="sequence">10</field>
        </record>

        <record id="rab_category_labor" model="kokai.rab.category">
            <field name="name">Tenaga Kerja</field>
            <field name="code">TK</field>
            <field name="sequence">20</field>
        </record>

        <record id="rab_category_equipment" model="kokai.rab.category">
            <field name="name">Peralatan</field>
            <field name="code">EQ</field>
            <field name="sequence">30</field>
        </record>

        <record id="rab_category_operational" model="kokai.rab.category">
            <field name="name">Operasional</field>
            <field name="code">OP</field>
            <field name="sequence">40</field>
        </record>

        <!-- Sub Categories -->
        <record id="rab_category_hardware" model="kokai.rab.category">
            <field name="name">Hardware</field>
            <field name="code">HW</field>
            <field name="parent_id" ref="rab_category_material"/>
            <field name="sequence">10</field>
        </record>

        <record id="rab_category_software" model="kokai.rab.category">
            <field name="name">Software</field>
            <field name="code">SW</field>
            <field name="parent_id" ref="rab_category_material"/>
            <field name="sequence">20</field>
        </record>
    </data>
</odoo>