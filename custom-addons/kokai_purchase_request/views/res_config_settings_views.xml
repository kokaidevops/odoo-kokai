<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
    
        <record id="res_config_settings_view_form_purchase_inherit_team" model="ir.ui.view">
            <field name="name">res.config.settings.view.form.inherit</field>
            <field name="model">res.config.settings</field>
            <field name="inherit_id" ref="purchase.res_config_settings_view_form_purchase"/>
            <field name="arch" type="xml">
                <xpath expr="//div[@name='matrix_setting_container']" position="after">
                    <h2>Other</h2>
                    <div class="row mt16 o_settings_container" name="other">
                        <div class="col-lg-6 col-12 o_setting_box" id="default_purchase_team">
                            <div class="o_setting_left_pane">
                            </div>
                            <div class="o_setting_right_pane">
                                <field name="default_purchase_team_id"/>
                                <div class="text-muted">
                                    Default Purchasing Team
                                </div>
                            </div>
                        </div>
                    </div>
                </xpath>
            </field>
        </record>
    
    </data>

</odoo>
