<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
    
        <record id="seq_vendor_offering" model="ir.sequence">
            <field name="name">Vendor Offering</field>
            <field name="code">vendor.offering</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
            <field name="number_next">1</field>
            <field name="use_date_range">1</field>
            <field name="range_reset">yearly</field>
            <field name="prefix">VDR-OFFER-%(year)s-</field>
        </record>

        <record id="seq_purchase_order_batch" model="ir.sequence">
            <field name="name">PO Batch</field>
            <field name="code">purchase.order.batch</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
            <field name="number_next">1</field>
            <field name="use_date_range">1</field>
            <field name="range_reset">yearly</field>
            <field name="prefix">PO-BATCH-%(year)s-</field>
        </record>
    
    </data>

</odoo>
